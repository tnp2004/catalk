---
import Layout from "../../layouts/Layout.astro";
import Card from "../../components/Card.astro";

const { PUBLIC_SERVER_API } = import.meta.env
const res: Promise<ResponseData<string[]>> = await(await fetch(`${PUBLIC_SERVER_API}/cats/breeds`)).json()
const breedsRes = await res
---

<Layout>
    <ul class="my-3 p-1 w-1/2 mx-auto flex flex-wrap">
        {breedsRes.data.map(v => <Card breed={v} />)}
    </ul>
</Layout>
